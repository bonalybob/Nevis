  "scripts": {
    "eslint:check": "vue-cli-service lint --no-fix",
    "eslint:fix": "vue-cli-service lint",
    "prettier:check": "prettier --check **",
    "prettier:fix": "prettier --write ** ",
    "lint": "npm run prettier:fix && npm run eslint:fix",
